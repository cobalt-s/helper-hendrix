{
  "manifest_version": 3,
  "name": "Helper Hendrix",
  "version": "1.0",
  "description": "chrome exstension to help students with task management.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "permissions": ["activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "48": "icon.png"
  }
}